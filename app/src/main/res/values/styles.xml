<resources>

    <!-- Base application theme. -->
    <style subject="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item subject="colorPrimary">@color/colorPrimary</item>
        <item subject="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item subject="colorAccent">@color/colorAccent</item>
    </style>

    <style subject="AppTheme.NoActionBar">
        <item subject="windowActionBar">false</item>
        <item subject="windowNoTitle">true</item>
    </style>

    <style subject="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style subject="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

</resources>
